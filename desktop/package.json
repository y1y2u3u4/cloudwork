{
  "name": "workany",
  "private": true,
  "version": "0.1.11",
  "type": "module",
  "scripts": {
    "ver": "./scripts/version.sh",
    "dev": "vite",
    "dev:api": "pnpm --filter workany-api dev",
    "predev:app": "node scripts/check-rust.js",
    "dev:app": "pnpm tauri dev",
    "dev:web": "pnpm dev",
    "dev:all": "concurrently \"pnpm dev:api\" \"pnpm dev:app\"",
    "build": "vite build",
    "build:api": "pnpm --filter workany-api build",
    "build:api:binary": "pnpm --filter workany-api build:binary",
    "build:api:binary:mac-arm": "pnpm --filter workany-api build:binary:mac-arm",
    "build:api:binary:mac-intel": "pnpm --filter workany-api build:binary:mac-intel",
    "build:api:binary:linux": "pnpm --filter workany-api build:binary:linux",
    "build:api:binary:windows": "pnpm --filter workany-api build:binary:windows",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "tauri": "tauri",
    "tauri:dev": "concurrently \"pnpm dev:api\" \"tauri dev\"",
    "tauri:build": "pnpm build:api:binary && tauri build",
    "tauri:build:linux": "pnpm build:api:binary:linux && tauri build --target x86_64-unknown-linux-gnu",
    "tauri:build:windows": "pnpm build:api:binary:windows && tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:mac-intel": "pnpm build:api:binary:mac-intel && tauri build --target x86_64-apple-darwin",
    "tauri:build:mac-arm": "pnpm build:api:binary:mac-arm && tauri build --target aarch64-apple-darwin",
    "build:app": "./scripts/build.sh",
    "build:app:with-claude": "./scripts/build.sh --with-claude",
    "build:app:mac-arm": "./scripts/build.sh mac-arm",
    "build:app:mac-arm:with-claude": "./scripts/build.sh mac-arm --with-claude",
    "build:app:mac-arm:release": "./scripts/build.sh mac-arm --with-claude --sign",
    "build:app:mac-intel": "./scripts/build.sh mac-intel",
    "build:app:mac-intel:with-claude": "./scripts/build.sh mac-intel --with-claude",
    "build:app:mac-intel:release": "./scripts/build.sh mac-intel --with-claude --sign",
    "build:app:linux": "./scripts/build.sh linux",
    "build:app:linux:with-claude": "./scripts/build.sh linux --with-claude",
    "build:app:windows": "./scripts/build.sh windows",
    "build:app:windows:with-claude": "./scripts/build.sh windows --with-claude"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-shell": "^2.3.4",
    "@tauri-apps/plugin-sql": "^2.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.12.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "4.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tauri-apps/cli": "^2",
    "@types/jszip": "^3.4.1",
    "@types/node": "^25.0.8",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.0.0",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
